let
  var n : Char;
  var guess : Char
in
  begin
    n := '7';
    put('G'); put('u'); put('e'); put('s'); put('s'); put(':');
    put(chr(10));

    repeat
      begin
        getchar(guess);
        put(guess)
      end
    until guess = n;

    put(chr(10));
    put('Y'); put('a'); put('y'); put('!')
  end